https://leetcode.com/problems/partitioning-into-minimum-number-of-deci-binary-numbers/

class Solution {
public:
    int minPartitions(string n) {
        int mx = 0;

        for(auto i:n)
        {
            if(mx < i) 
            {
                mx = i;
            }
            if(i == '9') return 9;
        }

        return (mx - '0');
         
    }
};

