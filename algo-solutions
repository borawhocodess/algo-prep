https://www.algoexpert.io/questions/find-kth-largest-value-in-bst

# This is an input class. Do not edit.
class BST:
    def __init__(self, value, left=None, right=None):
        self.value = value
        self.left = left
        self.right = right

def findKthLargestValueInBst(tree, k):
    stak = []
    while tree or stak:
        if tree is not None:
            stak.append(tree)
            print("append")
            tree = tree.right
        else:
            tree = stak.pop()
            print("pop")
            if k == 1:
                return tree.value
            else:
                k = k - 1
                tree = tree.left

